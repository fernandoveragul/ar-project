<CameraClick>:
    orientation: 'vertical'
    Camera:
        id: camera
        resolution: (640, 480)
        play: False
    MDRectangleFlatButton:
        id: toggle_button
        text: 'Включить'
        on_press: root.on_play()
        pos_hint: {"center_x": .5, "center_y": .5}
        height: '48dp'